  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'graph-rs'",
      "cargo": {
        "args": [
          "build",
          "--manifest-path=web-native/Cargo.toml",
          "--bin=graph-rs",
          "--package=graph-rs"
        ],
        "filter": {
          "name": "graph-rs",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/web-native"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'graph-rs'",
      "cargo": {
        "args": [
          "test",
          "--manifest-path=web-native/Cargo.toml",
          "--no-run",
          "--bin=graph-rs",
          "--package=graph-rs"
        ],
        "filter": {
          "name": "graph-rs",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}/web-native"
    }
  ]
}
